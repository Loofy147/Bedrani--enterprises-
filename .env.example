# .env.example - Environment variables for the PropTech Platform

# Common Database Settings (Development & Production)
POSTGRES_DB=proptech_db
POSTGRES_USER=proptech_user
POSTGRES_PASSWORD=your_secure_postgres_password

# Production Specific Settings (`docker-compose.prod.yml`)
# ---------------------------------------------------------

# Domains for frontend applications and API
DOMAIN_MAAMUR=bedrani-maamur.dz
DOMAIN_DJELALI=bedrani-djelali.dz
API_DOMAIN=api.bedrani-platform.dz

# Traefik Dashboard Settings
TRAEFIK_DASHBOARD_HOST=traefik.bedrani-platform.dz
# Generate a htpasswd string for basic auth.
# You can use an online generator or `htpasswd` command-line tool.
# Example for user 'admin' with password 'password': admin:$apr1$j1g.21j...
TRAEFIK_BASIC_AUTH_USERS=your_htpasswd_string_here

# ACME (Let's Encrypt) Settings for Traefik
ACME_EMAIL=your_email@example.com
